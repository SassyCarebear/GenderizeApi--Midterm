@using GenderizeApi__Midterm.Models
@model Gender
@{
    ViewData["Title"] = "GenderizedPage";
}
 <section class="intro">
    <h1>Welcome</h1>
    <h4>Welcome @Model.Data.name</h4> 
    <ul>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-controller="User" asp-action="AllTripsView" asp-route-agentID="@Model.UserID">View Booked Trips</a>
        </li>
    </ul>
  </section>
  <section>
      <h4>Your profile data</h4>
      <table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>

        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Data.FirstName</td>
            <td>@Model.Data.LastName</td>
            <td>@Model.Data.Email</td>
            <td>@Model.Data.Phone</td>
            <td>@Model.Data.Street1</td>
            <td>@Model.Data.Street2</td>
            <td>@Model.Data.City</td>
            <td>@Model.Data.State</td>
            <td>@Model.Data.ZipCode</td>
            <td>@Html.ActionLink("Edit", "Edit", "User", new {id = Model.Data.UserId})</td>
        </tr>

</tbody>
</table>
  </section>

  <section>
      <h4>Book a Trip</h4>
  </section>
